{
  "name": "lilis-kitchen",
  "version": "1.0.0",
  "description": "E-commerce website for lili's kitchen.",
  "engines": {
    "node": "16.14.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "start http://127.0.0.1:5173/ & npm run dev",
    "dev": "npx concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "lint": "eslint . --ext .js,.jsx"
  },
  "author": "Felix Willem",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "react-cookies": "^0.1.1"
  }
}
